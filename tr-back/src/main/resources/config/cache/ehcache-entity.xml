<?xml version="1.0" encoding="UTF-8"?>
<ehcache name="back-entity">
	<!-- 实体缓存，根据id 缓存 -->
    <diskStore path="java.io.tmpdir"/>

    <defaultCache
            maxEntriesLocalHeap="1000"
            eternal="false"
            timeToIdleSeconds="3600"
            timeToLiveSeconds="3600"
            overflowToDisk="false">
    </defaultCache>
    
    <cache name="com.tmser.tr.uc.bo.UserSpace"
           maxEntriesLocalHeap="20000"
           eternal="false"
           timeToIdleSeconds="360000"
           timeToLiveSeconds="360000"
           overflowToDisk="false">
    </cache>
    
    <cache name="com.tmser.tr.manage.meta.bo.Book"
           maxEntriesLocalHeap="20000"
           eternal="false"
           timeToIdleSeconds="360000"
           timeToLiveSeconds="360000"
           overflowToDisk="false">
    </cache>
    
    <cache name="com.tmser.tr.manage.meta.bo.Icon"
           maxEntriesLocalHeap="1000"
           eternal="true"
           overflowToDisk="false">
    </cache>
    
   <cache name="com.tmser.tr.manage.meta.bo.SysDic"
           maxEntriesLocalHeap="2000"
           eternal="true"
           overflowToDisk="false">
    </cache>
        
   <cache name="com.tmser.tr.manage.org.bo.Area"
           maxEntriesLocalHeap="20000"
           eternal="true"
           overflowToDisk="false">
    </cache>
    
</ehcache>
